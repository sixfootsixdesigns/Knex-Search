{
  "name": "knex-search",
  "version": "1.0.0",
  "description": "knex search utility",
  "license": "MIT",
  "author": "James Olson",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write \"src/**/*.{ts,json,js}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,json,js}\"",
    "lint": "tslint --project .",
    "test": "mocha --opts ./mocha.opts $(find src -name '*.spec.ts')",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "knex": "^0.16.3",
    "pg": "^7.8.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/knex": "^0.15.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2"
  }
}
